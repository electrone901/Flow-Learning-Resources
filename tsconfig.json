{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "components/Navbar.js",
    "components/Explore.js",
    "components/withTransition.js",
    "components/Landing.js",
    "components/CreateContentFirstPart.js",
    "components/PartnerCard.js",
    "components/RewardPill.js",
    "pages/community.js"
  ],
  "exclude": ["node_modules"]
}
